
{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/status",
      "dest": "src/index.ts"
    },
    {
      "src": "/trips(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/saccos(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/operators(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
